    {
        "settings": [ "environment:development" ],
        "staticHandling": {
            "forceUpdate": true
        },
        "deployRuntimeClient": {
            "useCompression": false,
            "useDedupe": true,
            "debug": true,
            "yuiPrefix": "YY",
            "mojitoPrefix": "ZZ",
            "exclude": [
                "log"
            ]
        }
    },
    {
        "settings": [ "environment:prod" ],
        "log": {
            "server": {
                "level": "error",
                "yui": false
            },
            "client": {
                "level": "error",
                "yui": false
            }
        },


        "shareYUIInstance": true,
        "cacheViewTemplates": true,
        "embedJsFilesInHtmlFrame": false,
        "deployRuntimeClient": {
            "useCompression": false,
            "useDedupe": false,
            "exclude": [
                "log",
                "appspecificconfig"
            ]
        }


